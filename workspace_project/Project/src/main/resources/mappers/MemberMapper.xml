<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.m4gi.mapper.MemberMapper">

    <select id="findByEmail" resultType="com.example.dto.MemberDTO" parameterType="String">
        SELECT user_id as userId,
               email as userEmail,
               username as userName
        FROM users
        WHERE email = #{email}
    </select>


</mapper>
